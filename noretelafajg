
x = input ("Eastbound?")
y = input ("Westbound?")
if x == "true" or "True" and y=="False" or "false":
    x= True and print("There is east bound traffic.")
    if x== True and y== False:
        print ("There is no west bound traffic")
elif y == "True"or "true" and x== "False" or "false":
    y= True and print ("There is west bound traffic.")
    if y== True and x== False:
        print ("There is no east bound traffic.")
else:
    if x and y== True:
        print("False")
    elif x and y== False:
        print("True")